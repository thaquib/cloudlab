apiVersion: apps/v1
kind: Deployment
metadata:
  name: demo
spec:
  selector:
    matchLabels:
      run: demo
  replicas: 4
  template:
    metadata:
      labels:
        run: demo
    spec:
      containers:
        - name: demo
          image: kubernetes-project-demo:1.0
          ports:
            - containerPort: 80
